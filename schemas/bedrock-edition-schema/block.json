{
    "id": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/block.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "VanillaTags": {
            "description": "built-in tag. https://wiki.bedrock.dev/blocks/block-tags.html#vanilla-tags",
            "type": "object",
            "default": {}
        },
        "materialInstances": {
            "description": "A material instance definition to map to a material instance in a geometry file. The material instance '*' will be used for any materials that don't have a match.",
            "required": [
                "texture"
            ],
            "properties": {
                "texture": {
                    "description": "Texture name for the material.",
                    "type": "string"
                },
                "render_method": {
                    "description": "The render method to use. Must be one of 'opaque', 'double_sided', 'blend', or 'alpha_test'.",
                    "enum": [
                        "double_sided",
                        "blend",
                        "opaque",
                        "alpha_test"
                    ],
                    "default": "opaque"
                },
                "face_dimming": {
                    "description": "Should this material be dimmed by the direction it's facing.",
                    "default": true,
                    "type": "boolean"
                },
                "ambient_occlusion": {
                    "description": "Should this material have ambient occlusion applied when lighting.",
                    "default": true,
                    "type": "boolean"
                }
            }
        },
        "allowedBlockFaces": {
            "enum": [
                "all",
                "north",
                "south",
                "west",
                "east",
                "up",
                "down",
                "side"
            ]
        }
    },
    "properties": {
        "minecraft:block": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "object",
                    "properties": {
                        "identifier": {
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceIDBlock"
                        },
                        "properties": {
                            "type": "object",
                            "description": "The indexed properties for this block. block_properties. like blockstates.",
                            "additionalProperties": {
                                "description": "Property name. must contain a namespace example; 'custom:weardo_direction'",
                                "type": "array",
                                "uniqueItems": true,
                                "maxItems": 16,
                                "minItems": 2,
                                "items": {
                                    "description": "A list of this blocks valid properties.",
                                    "anyOf": [
                                        {
                                            "type": "number"
                                        },
                                        {
                                            "type": "string",
                                            "pattern": "^[^\\s]*$"
                                        },
                                        {
                                            "type": "boolean"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "components": {
                    "type": "object",
                    "properties": {
                        "minecraft:aim_collision": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "The outline when you are looking at the block.",
                            "default": false,
                            "anyOf": [
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "object",
                                    "required": [
                                        "origin",
                                        "size"
                                    ],
                                    "properties": {
                                        "origin": {
                                            "type": "array",
                                            "default": [
                                                -8.0,
                                                0.0,
                                                -8.0
                                            ],
                                            "description": "Minimal position Bounds of the collision box. Origin can't be smaller than (-8, 0, -8) and can't be more than (8, 16, 8)."
                                        },
                                        "size": {
                                            "type": "array",
                                            "default": [
                                                16.0,
                                                16.0,
                                                16.0
                                            ],
                                            "description": "Size of each side of the box of the component. When added to origin it can't make origin smaller than (-8, 0, -8) or more than (0, 16, 8)."
                                        }
                                    }
                                }
                            ]
                        },
                        "minecraft:block_collision": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "Describes the collision of the block with entities and actors. If set to false, it disables the collision of the block with entities. An origin of [-8.0, 0.0, -8.0] with a size of [16, 16, 16] is a unit cube.",
                            "default": false,
                            "anyOf": [
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "object",
                                    "required": [
                                        "origin",
                                        "size"
                                    ],
                                    "properties": {
                                        "origin": {
                                            "type": "array",
                                            "default": [
                                                -8.0,
                                                0.0,
                                                -8.0
                                            ],
                                            "description": "Minimal position Bounds of the collision box"
                                        },
                                        "size": {
                                            "type": "array",
                                            "default": [
                                                16.0,
                                                16.0,
                                                16.0
                                            ],
                                            "description": "Size of each side of the box of the component"
                                        }
                                    }
                                }
                            ]
                        },
                        "minecraft:block_light_emission": {
                            "title": "STABLE",
                            "description": "The amount of light this block will emit in a range [0.0, 1.0].",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/Decimal",
                            "maximum": 1.0
                        },
                        "minecraft:block_light_filter": {
                            "title": "STABLE",
                            "description": "The amount of light this block will filter out. Higher value means more light will be filtered out (0 - 15).",
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 15,
                            "default": 0
                        },
                        "minecraft:breathability": {
                            "title": "STABLE",
                            "description": "The breathing type of this block that affects the breathing state of mobs when they have their breathing points inside this block. Available values: solid, air",
                            "enum": [
                                "solid",
                                "air"
                            ],
                            "default": "solid"
                        },
                        "minecraft:crafting_table": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "Describes the component of a custom crafting table. This component supports only \"recipe_shaped\" and \"recipe_shapeless\" typed recipes and not others like \"recipe_furnace\" or \"recipe_brewing_mix\". If there are two recipes for one item, the recipe book will pick the first that was parsed. If two input recipes are the same, crafting may assert and the resulting item may vary.",
                            "type": "object",
                            "properties": {
                                "grid_size": {
                                    "type": "number",
                                    "minimum": 3,
                                    "maximum": 3,
                                    "description": "The size of the crafting grid, Currently only supports '3'"
                                },
                                "custom_description": {
                                    "type": "string",
                                    "description": "Defines the name in the UI of this table. If not specified, the name of the block will be used."
                                },
                                "crafting_tags": {
                                    "type": "array",
                                    "description": "Defines the tags recipes should define to be crafted on this table.",
                                    "items": {
                                        "type": "string"
                                    },
                                    "maxItems": 64
                                }
                            }
                        },
                        "minecraft:creative_category": {
                            "title": "STABLE",
                            "description": "Specifies the creative group for the block.",
                            "default": {},
                            "anyOf": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "category": {
                                            "description": "The tab in the creative invetory to place this block.",
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemTab"
                                        },
                                        "group": {
                                            "description": "The group in the category tab to place this block.",
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemGroup"
                                        }
                                    }
                                },
                                {
                                    "type": "string",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemGroup"
                                }
                            ]
                        },
                        "minecraft:destroy_time": {
                            "title": "STABLE",
                            "description": "Sets the destroy time property for the block. Greater numbers result in greater mining times.",
                            "type": "number",
                            "default": 0.0,
                            "minimum": 0.000000,
                            "maximum": 340282346638528859811704183484516925440.000000
                        },
                        "minecraft:display_name": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "Specifies the language file key that maps to what text will be displayed when you hover over the block.",
                            "type": "string"
                        },
                        "minecraft:explosion_resistance": {
                            "title": "STABLE",
                            "description": "Sets the explosion resistance for this block.",
                            "type": "number",
                            "default": 0.0,
                            "minimum": 0.0
                        },
                        "minecraft:flammable": {
                            "title": "STABLE",
                            "description": "Describes the flammable properties for this block.",
                            "type": "object",
                            "required": [
                                "burn_odds",
                                "flame_odds"
                            ],
                            "properties": {
                                "burn_odds": {
                                    "type": "integer",
                                    "minimum": 0,
                                    "default": 0,
                                    "description": "How likely the block will be destroyed by flames when on fire."
                                },
                                "flame_odds": {
                                    "type": "integer",
                                    "minimum": 0,
                                    "default": 0,
                                    "description": "How likely the block will be destroyed by flames when on fire.How likely the block will catch flame when next to a fire."
                                }
                            }
                        },
                        "minecraft:friction": {
                            "title": "STABLE",
                            "description": "Property describing the friction for this block in a range of [0.1, 1.0]. Friction affects an entity's movement speed when it travels on the block. Greater value results in less friction.",
                            "minimum": 0.1,
                            "maximum": 1.0,
                            "default": 0.6
                        },
                        "minecraft:geometry": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "The geometry definition name to use. Must have \"geometry.\" at the beginning.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                        },
                        "minecraft:loot": {
                            "title": "STABLE",
                            "description": "The loot table to run when the player breaks the block. ex; \"loot_tables/blocks/simple_dungeon.json\"",
                            "type": "string",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/common_regex.json#/definitions/file_path_json",
                            "examples": [
                                "loot_tables/blocks/BLOCK.json"
                            ]
                        },
                        "minecraft:map_color": {
                            "title": "STABLE",
                            "description": "A color represented as a hex value. This will be the color rendered to a map.",
                            "type": "string",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/common_regex.json#/definitions/hex_color"
                        },
                        "minecraft:material_instances": {
                            "title": "STABLE",
                            "description": "The material instances for a block.Maps face or material_instance names in a geometry file to an actual material instance. Material instance can either be a full material instance or a name to another already defined instance.",
                            "type": "object",
                            "additionalProperties": {
                                "$ref": "#/definitions/materialInstances"
                            }
                        },
                        "minecraft:part_visibility": {
                            "title": "EXPERIMENTAL (toggle: Upcomming Creator Features)",
                            "description": "Maps bone names in a geometry file to a condition that turns their rendering on/off. The condition should be a Molang query that uses block properties to determine true/false Supported queries include 'has_block_property', 'block_property', and other queries that can evaluate without knowledge of the block's in-game positional or player affected data.",
                            "type": "object",
                            "additionalProperties": {
                                "description": "The bone id to render true/false",
                                "type": "string",
                                "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                            }
                        },
                        "minecraft:placement_filter": {
                            "title": "STABLE",
                            "description": "Sets rules for under what conditions the block can be placed/survive",
                            "type": "object",
                            "properties": {
                                "conditions": {
                                    "type": "array",
                                    "description": "List of conditions where the block can be placed/survive",
                                    "items": {
                                        "properties": {
                                            "allowed_faces": {
                                                "description": "List of any of the following strings: up, down, north, south, east, west, side, all",
                                                "items": {
                                                    "$ref": "#/definitions/allowedBlockFaces"
                                                }
                                            },
                                            "block_filter": {
                                                "description": "List of blocks (can use tags to specify them) that this block can be placed against in the allowed_faces direction",
                                                "type": "array",
                                                "items": {
                                                    "anyOf": [
                                                        {
                                                            "type": "string",
                                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceIDBlock"
                                                        },
                                                        {
                                                            "type": "object",
                                                            "properties": {
                                                                "tags": {
                                                                    "description": "Molang",
                                                                    "type": "string",
                                                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                                                }
                                                            }
                                                        }
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "minecraft:random_ticking": {
                            "title": "STABLE",
                            "description": "Describes the component that will trigger an even at a regular interval between two values",
                            "type": "object",
                            "required": [
                                "on_tick"
                            ],
                            "properties": {
                                "on_tick": {
                                    "type": "object",
                                    "required": [
                                        "event"
                                    ],
                                    "properties": {
                                        "range": {
                                            "description": "The Range between which the component will trigger his event.",
                                            "type": "array",
                                            "default": [
                                                10,
                                                10
                                            ],
                                            "items": {
                                                "type": "integer"
                                            },
                                            "minItems": 2,
                                            "maxItems": 2
                                        },
                                        "condition": {
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang",
                                            "default": 1,
                                            "description": "The condition of event to be executed on the block."
                                        },
                                        "event": {
                                            "type": "string",
                                            "default": "set_block_property",
                                            "description": "The type of event executed on the block."
                                        },
                                        "target": {
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/eventTarget",
                                            "default": "self",
                                            "description": "The target of event executed on the block."
                                        }
                                    }
                                }
                            }
                        },
                        "minecraft:rotation": {
                            "title": "STABLE",
                            "description": "This is the block's rotation around the center of the cube in degrees. The rotation order is x-y-z.",
                            "type": "array",
                            "default": [
                                0,
                                0,
                                0
                            ]
                        },
                        "minecraft:ticking": {
                            "title": "STABLE",
                            "description": "Describes the component that will trigger an even at a regular interval between two values",
                            "type": "object",
                            "required": [
                                "looping",
                                "range",
                                "on_tick"
                            ],
                            "properties": {
                                "looping": {
                                    "type": "boolean",
                                    "default": true,
                                    "description": "Does the event loop"
                                },
                                "range": {
                                    "type": "array",
                                    "default": [
                                        10,
                                        10
                                    ],
                                    "items": {
                                        "type": "integer"
                                    },
                                    "minItems": 2,
                                    "maxItems": 2,
                                    "description": "The Range between which the component will trigger his event."
                                },
                                "on_tick": {
                                    "type": "object",
                                    "required": [
                                        "event"
                                    ],
                                    "properties": {
                                        "condition": {
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang",
                                            "default": 1,
                                            "description": "The condition of event to be executed on the block."

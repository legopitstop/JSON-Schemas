                                        },
                                        "target": {
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/eventTarget",
                                            "default": "self",
                                            "description": "The target of event executed on the block."
                                        }
                                    }
                                }
                            }
                        },

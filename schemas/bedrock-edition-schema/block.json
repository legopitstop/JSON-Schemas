{
    "id": "https://legopitstop.github.io/JSON-Schemas/schemas/bedrock-edition-schema/block.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "VanillaTags": {
            "description": "built-in tag. https://wiki.bedrock.dev/blocks/block-tags.html#vanilla-tags",
            "type": "object",
            "default": {}
        },
        "materialInstances": {
            "required": [
                "texture"
            ],
            "properties": {
                "texture": {
                    "description": "Texture name referenced in terrain_texture",
                    "type": "string"
                },
                "render_method": {
                    "description": "The render method to use. \n'double_sided' double sided is a blending material without “backface culling”.\n'blend' Used for a block like stained glass. Allows for transparency and translucency (slightly transparent pixels).\n'opaque' Used for a normal block texture without an alpha layer. Does not allow for transparency or translucency \n'alpha_test' Used for a block like the vanilla (unstained) glass. Does not allow for translucency, only either fully opaque or fully transparent pixels.",
                    "enum": [
                        "double_sided",
                        "blend",
                        "opaque",
                        "alpha_test"
                    ]
                },
                "face_dimming": {
                    "description": "Refers to whether or not block is affected by lighting",
                    "default": true,
                    "type": "boolean"
                },
                "ambient_occlusion": {
                    "description": "Refers to whether or not faces have smooth lighting",
                    "default": true,
                    "type": "boolean"
                }
            }
        },
        "allowedBlockFaces": {
            "enum": [
                "all",
                "north",
                "south",
                "west",
                "east",
                "up",
                "down",
                "side"
            ]
        }
    },
    "properties": {
        "minecraft:block": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "object",
                    "properties": {
                        "identifier": {
                            "$ref": "https://legopitstop.github.io/JSON-Schemas/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceIDBlock"
                        },
                        "properties": {
                            "type": "object",
                            "description": "The indexed properties for this block. block_properties. like blockstates.",
                            "additionalProperties": {
                                "description": "Property name. must contain a namespace example; 'custom:weardo_direction'",
                                "type": "array",
                                "uniqueItems": true,
                                "maxItems": 16,
                                "minItems": 2,
                                "items": {
                                    "description": "A list of this blocks valid properties.",
                                    "anyOf": [
                                        {
                                            "type": "number"
                                        },
                                        {
                                            "type": "string",
                                            "pattern": "^[^\\s]*$"
                                        },
                                        {
                                            "type": "boolean"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "components": {
                    "type": "object",
                    "properties": {
                        "minecraft:loot": {
                            "title": "STABLE",
                            "description": "The loot table to run when the player breaks the block. ex; \"loot_tables/blocks/simple_dungeon.json\"",

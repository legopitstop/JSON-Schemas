{
    "id": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/block.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "VanillaTags": {
            "description": "Vanilla tags can be applied to custom blocks, and some vanilla blocks are tagged internally. This kind of tag doesn't give blocks vanilla features. The only usage of it is to tag blocks to make them have the same tags as the vanilla blocks for queries or tests. https://wiki.bedrock.dev/blocks/block-tags.html#vanilla-tags",
            "type": "object",
            "default": {}
        },
        "VanillaBlockTags": {
            "description": "Vanilla block tags are tags built specifically for blocks. They can give some vanilla features to the blocks which are tagged with them.. https://wiki.bedrock.dev/blocks/block-tags.html#vanilla-tags",
            "type": "object",
            "default": {}
        },
        "materialInstances": {
            "description": "A material instance definition to map to a material instance in a geometry file. The material instance '*' will be used for any materials that don't have a match.",
            "required": [
                "texture"
            ],
            "properties": {
                "ambient_occlusion": {
                    "description": "Should this material have ambient occlusion applied when lighting? If true, shadows will be created around and underneath the block.",
                    "default": true,
                    "type": "boolean"
                },
                "face_dimming": {
                    "description": "Should this material be dimmed by the direction it's facing?",
                    "default": true,
                    "type": "boolean"
                },
                "render_method": {
                    "description": "The render method to use. Must be one of these options:\n'opaque' - Used for a regular block texture without an alpha layer. Does not allow for transparency or translucency. (Allows redstone to pass through it, grass decay, and allow mobs to spawn on it)\n'double_sided' - Used for completely disabling backface culling.\n'blend' - Used for a block like stained glass. Allows for transparency and translucency (slightly transparent textures) (Allows redstone to pass through it, it won't make grass decay, and it won't allow mobs to spawn on it).\n'alpha_test' - Used for a block like the vanilla (unstained) glass. Does not allow for translucency, only fully opaque or fully transparent textures. Also disables backface culling. (Same props as blend)",
                    "enum": [
                        "double_sided",
                        "blend",
                        "opaque",
                        "alpha_test"
                    ],
                    "default": "opaque"
                },
                "texture": {
                    "description": "Texture name for the material.",
                    "type": "string"
                }
            }
        },
        "allowedBlockFaces": {
            "enum": [
                "all",
                "north",
                "south",
                "west",
                "east",
                "up",
                "down",
                "side"
            ]
        }
    },
    "properties": {
        "minecraft:block": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "object",
                    "properties": {
                        "identifier": {
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceIDBlock"
                        },
                        "properties": {
                            "type": "object",
                            "description": "The indexed properties for this block. block_properties. like blockstates.",
                            "additionalProperties": {
                                "description": "Property name. must contain a namespace example; 'custom:weardo_direction'",
                                "type": "array",
                                "uniqueItems": true,
                                "maxItems": 16,
                                "minItems": 2,
                                "items": {
                                    "description": "A list of this blocks valid properties.",
                                    "type": [
                                        "string",
                                        "number",
                                        "boolean",
                                        "null"
                                    ]
                                }
                            }
                        }
                    }
                },
                "components": {
                    "type": "object",
                    "properties": {
                        "minecraft:aim_collision": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "Describes the collision of the block for raycast and its outline. If set to false it disables the collision of the block with entities. An origin of [-8.0, 0.0, -8.0] with a size of [16, 16, 16] is a unit cube.",
                            "default": false,
                            "anyOf": [
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "object",
                                    "required": [
                                        "origin",
                                        "size"
                                    ],
                                    "properties": {
                                        "origin": {
                                            "type": "array",
                                            "default": [
                                                -8.0,
                                                0.0,
                                                -8.0
                                            ],
                                            "description": "Minimal position Bounds of the selection box. Origin is specified as [x, y, z] and must be in the range (-8, 0, -8) to (8, 16, 8), inclusive"
                                        },
                                        "size": {
                                            "type": "array",
                                            "default": [
                                                16.0,
                                                16.0,
                                                16.0
                                            ],
                                            "description": "Size of each side of the selection box. Size is specified as [x, y, z]. Origin + size must be in the range (-8, 0, -8) to (8, 16, 8), inclusive"
                                        }
                                    }
                                }
                            ]
                        },
                        "minecraft:block_light_emission": {
                            "title": "STABLE",
                            "description": "The amount of light this block will emit in a range [0.0, 1.0].",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/Decimal",
                            "maximum": 1.0
                        },
                        "minecraft:block_light_filter": {
                            "title": "STABLE",
                            "description": "The amount that light will be dampened when it passes through the block, in a range (0-15). Higher value means the light will be dampened more.",
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 15,
                            "default": 15
                        },
                        "minecraft:breathability": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "The breathing type of this block that affects the breathing state of mobs when they have their breathing points inside this block. Available values: solid, air",
                            "enum": [
                                "solid",
                                "air"
                            ],
                            "default": "solid"
                        },
                        "minecraft:collision_box": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "Defines the area of the block that collides with entities. If set to true, default values are used. If set to false, the block's collision with entities is disabled. If this component is omitted, default values are used.",
                            "default": false,
                            "anyOf": [
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "object",
                                    "required": [
                                        "origin",
                                        "size"
                                    ],
                                    "properties": {
                                        "origin": {
                                            "description": "Minimal position Bounds of the collision box. Origin is specified as [x, y, z] and must be in the range (-8, 0, -8) to (8, 16, 8), inclusive",
                                            "type": "array",
                                            "default": [
                                                -8.0,
                                                0.0,
                                                -8.0
                                            ]
                                        },
                                        "size": {
                                            "description": "Size of each side of the collision box. Size is specified as [x, y, z]. Origin + size must be in the range (-8, 0, -8) to (8, 16, 8), inclusive",
                                            "type": "array",
                                            "default": [
                                                16.0,
                                                16.0,
                                                16.0
                                            ]
                                        }
                                    }
                                }
                            ]
                        },
                        "minecraft:crafting_table": {
                            "title": "EXPERIMENTAL (Holiday Creator Features)",
                            "description": "Describes the component of a custom crafting table. This component supports only \"recipe_shaped\" and \"recipe_shapeless\" typed recipes and not others like \"recipe_furnace\" or \"recipe_brewing_mix\". If there are two recipes for one item, the recipe book will pick the first that was parsed. If two input recipes are the same, crafting may assert and the resulting item may vary.",
                            "type": "object",
                            "properties": {
                                "table_name": {
                                    "type": "string",
                                    "description": "Specifies the language file key that maps to what text will be displayed in the UI of this table. If the string given can not be resolved as a loc string, the raw string given will be displayed. If this field is omitted, the name displayed will default to the name specified in the display_name component. If this block has no display_name component, the name displayed will default to the name of the block."
                                },
                                "crafting_tags": {
                                    "type": "array",
                                    "description": "Defines the tags recipes should define to be crafted on this table. Limited to 64 tags. Each tag is limited to 64 characters.",
                                    "items": {
                                        "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/recipeTags"
                                    },
                                    "maxItems": 64
                                }
                            }
                        },
                        "minecraft:creative_category": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "Specifies the menu category and group for the block, which determine where this block is placed in the inventory and crafting table container screens. If this component is omitted, the block will not appear in the inventory or crafting table container screens.",
                            "default": {},
                            "anyOf": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "category": {
                                            "description": "Determines which category this block will be placed under in the inventory and crafting table container screens. Options are \"construction\", \"nature\", \"equipment\", \"items\", and \"none\". If omitted or \"none\" is specified, the block will not appear in the inventory or crafting table container screens.",
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemTab"
                                        },
                                        "group": {
                                            "description": "Specifies the language file key that maps to which expandable/collapsible group this block will be a part of within a category. If the string given can not be resolved as a loc string, then we will check if there is an existing group whose name matches the raw string. If this field is omitted, or there is no group whose name matches the loc string or the raw string, this block will be placed standalone in the given category.",
                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemGroup"
                                        }
                                    }
                                },
                                {
                                    "type": "string",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/itemGroup"
                                }
                            ]
                        },
                        "minecraft:destroy_time": {
                            "title": "STABLE",
                            "description": "Sets the destroy time property for the block. Greater numbers result in greater mining times. Time is measured in seconds with base equipment.",
                            "type": "number",
                            "default": 0.0,
                            "minimum": 0.000000,
                            "maximum": 340282346638528859811704183484516925440.000000
                        },
                        "minecraft:display_name": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "Specifies the language file key that maps to what text will be displayed when you hover over the block. Key is limited to 256 characters.",
                            "type": "string",
                            "maxLength": 256
                        },
                        "minecraft:explosion_resistance": {
                            "title": "STABLE",
                            "description": "How much power it can resist from explosion. Bigger the value is, more explosion power it blocks from explosion. If the value is less than remaining power from explosion, the block would destroy.",
                            "type": "number",
                            "default": 0.0,
                            "minimum": 0.0
                        },
                        "minecraft:flammable": {
                            "title": "STABLE",
                            "description": "Describes the flammable properties for this block. If set to true, default values are used. If set to false, this block will not be able to catch on fire. If this component is omitted, the block will not be able to catch on fire naturally from neighbors or lava, but it can still be directly ignited, and that fire will be able to spread to neighbor blocks.",
                            "type": "object",
                            "required": [
                                "burn_odds",
                                "flame_odds"
                            ],
                            "properties": {
                                "catch_chance_modifier": {
                                    "description": "A modifier affecting the chance that this block will catch flame when next to a fire. Values are greater than or equal to 0, with a higher number meaning more likely to catch on fire. The default value of 5 is the same as that of Planks.",
                                    "type": "integer",
                                    "default": 5
                                },
                                "destroy_chance_modifier": {
                                    "description": "A modifier affecting the chance that this block will be destroyed by flames when on fire. Values are greater than or equal to 0, with a higher number meaning more likely to be destroyed by fire. The default value of 20 is the same as that of Planks.",
                                    "type": "integer",
                                    "default": 20
                                }
                            }
                        },
                        "minecraft:friction": {
                            "title": "STABLE",
                            "description": "Property describing the friction for this block in a range of [0.1, 1.0]. Friction affects an entity's movement speed when it travels on the block. Greater value results in less friction.",
                            "minimum": 0.1,
                            "maximum": 1.0,
                            "default": 0.6
                        },
                        "minecraft:geometry": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "The geometry description identifier to use, this identifier must match an existing geometry identifier in any of the currently loaded resource packs.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                        },
                        "minecraft:loot": {
                            "title": "STABLE",
                            "description": "The path to the loot table, relative to the behavior pack. Path string is limited to 256 characters.",
                            "type": "string",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/common_regex.json#/definitions/file_path_json",
                            "examples": [
                                "loot_tables/blocks/BLOCK.json"
                            ],
                            "maxLength": 256
                        },
                        "minecraft:map_color": {
                            "title": "STABLE",
                            "description": "Sets the color of the block when rendered to a map. The color is represented as a hex value in the format \"#RRGGBB\". May also be expressed as an array of [R, G, B] from 0 to 255. If this component is omitted, the block will not show up on the map.",
                            "anyOf": [
                                {
                                    "type": "string",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/common_regex.json#/definitions/hex_color"
                                },
                                {
                                    "type": "array",
                                    "minItems": 3,
                                    "maxItems": 3,
                                    "items": {
                                        "description": "R, G, B",
                                        "type": "integer",
                                        "minimum": 0,
                                        "maximum": 255
                                    }
                                }
                            ]
                        },
                        "minecraft:material_instances": {
                            "title": "STABLE",
                            "description": "The material instances for a block. Maps face or material_instance names in a geometry file to an actual material instance. You can assign a material instance object to any of these faces: 'up', 'down', 'north', 'south', 'east', 'west', or '*'. You can also give an instance the name of your choosing such as 'my_instance', and then assign it to a face by doing 'north':'my_instance'.",
                            "type": "object",
                            "additionalProperties": {
                                "$ref": "#/definitions/materialInstances"
                            }
                        },
                        "minecraft:part_visibility": {
                            "title": "EXPERIMENTAL (toggle: Upcomming Creator Features)",
                            "description": "Sets conditions for when the block's different parts are visible.",
                            "type": "object",
                            "properties": {
                                "conditions": {
                                    "description": "A JSON object that contains a list of key/value pairs that map from bone name in a geometry file (key) to a condition that turns their rendering on/off (value). The condition should be a Molang query that uses block properties to determine true/false. Supported queries include 'has_block_property', 'block_property', and other queries that can evaluate without knowledge of the block's in-game positional or player affected data.",
                                    "type": "object",
                                    "default": {},
                                    "additionalProperties": {
                                        "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                    }
                                }
                            }
                        },
                        "minecraft:placement_filter": {
                            "title": "STABLE",
                            "description": "Sets rules for under what conditions the block can be placed/survive",
                            "type": "object",
                            "properties": {
                                "conditions": {
                                    "type": "array",
                                    "description": "List of conditions where the block can be placed/survive",
                                    "items": {
                                        "properties": {
                                            "allowed_faces": {
                                                "description": "List of any of the following strings: up, down, north, south, east, west, side, all. Limited to 6 faces.",
                                                "items": {
                                                    "$ref": "#/definitions/allowedBlockFaces"
                                                },
                                                "uniqueItems": true,
                                                "maxItems": 6
                                            },
                                            "block_filter": {
                                                "description": "List of blocks (can use tags to specify them) that this block can be placed against in the allowed_faces direction. Limited to 64 blocks.",
                                                "type": "array",
                                                "maxItems": 64,
                                                "items": {
                                                    "anyOf": [
                                                        {
                                                            "type": "string",
                                                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceIDBlock"
                                                        },
                                                        {
                                                            "type": "object",
                                                            "properties": {
                                                                "tags": {
                                                                    "description": "Molang",
                                                                    "type": "string",
                                                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                                                }
                                                            }
                                                        }
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "minecraft:queued_ticking": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "Triggers the specified event, either once, or at a regular interval equal to a number of ticks randomly chosen from the interval_range provided.",
                            "type": "object",
                            "required": [
                                "looping",
                                "range",
                                "on_tick"
                            ],
                            "properties": {
                                "interval_range": {
                                    "description": "A range of values, specified in ticks, that will be used to decide the interval between times this event triggers. Each interval will be chosen randomly from the range, so the times between this event triggering will differ given an interval_range of two different values. If the values in the interval_range are the same, the event will always be triggered after that number of ticks.",
                                    "type": "array",
                                    "default": [
                                        10,
                                        10
                                    ],
                                    "items": {
                                        "type": "integer"
                                    },
                                    "minItems": 2,
                                    "maxItems": 2
                                },
                                "looping": {
                                    "description": "Does the event loop? If false, the event will only be triggered once, after a delay equal to a number of ticks randomly chosen from the interval_range. If true, the event will loop, and each interval between events will be equal to a number of ticks randomly chosen from the interval_range.",
                                    "type": "boolean",
                                    "default": true
                                },
                                "on_tick": {
                                    "description": "The event that will be triggered once or on an interval.",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                }
                            }
                        },
                        "minecraft:random_ticking": {
                            "title": "STABLE",
                            "description": "Triggers the specified event randomly based on the random tick speed gamerule. The random tick speed determines how often blocks are updated. Some other examples of game mechanics that use random ticking are crop growth and fire spreading.",
                            "type": "object",
                            "required": [
                                "on_tick"
                            ],
                            "properties": {
                                "on_tick": {
                                    "description": "The event that will be triggered on random ticks.",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                }
                            }
                        },
                        "minecraft:rotation": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "This is the block's rotation around the center of the cube in degrees. The rotation order is x-y-z. Angles need to be in factors of 90.",
                            "type": "array",
                            "default": [
                                0,
                                0,
                                0
                            ],
                            "items": {
                                "type": "integer",
                                "multipleOf": 90
                            },
                            "minItems": 3,
                            "maxItems": 3
                        },
                        "minecraft:unit_cube": {
                            "title": "EXPERIMENTAL (toggle: Holiday Creator Features)",
                            "description": "Specifies that a unit cube is to be used with tessellation.",
                            "type": "object",
                            "default": {}
                        },
                        "minecraft:unwalkable": {
                            "title": "STABLE",
                            "description": "Sets the block as unwalkable. Mobs would not attempt to path over top of it when the value is set to true.",
                            "type": "boolean",
                            "default": false
                        },
                        "minecraft:on_fall_on": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "type": "object",
                            "required": [
                                "min_fall_distance",
                                "event"
                            ],
                            "properties": {
                                "condition": {
                                    "description": "The condition of event to be executed on the block.",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                                },
                                "event": {
                                    "description": "The event executed on the block.",
                                    "type": "string"
                                },
                                "min_fall_distance": {
                                    "description": "The minimum distance in blocks that an actor needs to fall to trigger this event.",
                                    "type": "integer",
                                    "default": 0.0
                                },
                                "target": {
                                    "description": "The target of event executed on the block.",
                                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/eventTarget",
                                    "default": "self"
                                }
                            }
                        },
                        "minecraft:on_interact": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },
                        "minecraft:on_placed": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },
                        "minecraft:on_player_destroyed": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },
                        "minecraft:on_player_placing": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },
                        "minecraft:on_step_off": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "type": "object",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },
                        "minecraft:on_step_on": {
                            "title": "STABLE",
                            "description": "Describes event for this block.",
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/EventTrigger"
                        },

{
    "id": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "minecraft:conditional_list",
    "description": "Feature type 'minecraft:conditional_list' has not yet been documented.",
    "properties": {
        "description": {
            "type": "object",
            "properties": {
                "identifier": {
                    "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/nameSpaceID"
                }
            }
        },
        "conditional_features": {
            "description": "Array of Features, and their associated Conditions, for attempted placement. These features will be evaluated as ordered.",
            "type": "array",
            "items": {
                "type": "object",
                "default": {},
                "properties": {
                    "places_feature": {
                        "description": "Feature to be placed.",
                        "type": "string"
                    },
                    "condition": {
                        "description": "Condition for placing associated Feature",
                        "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock-edition-schema/common.json#/definitions/MoLang"
                    }
                }
            }
        },
        "early_out_scheme": {
            "enum": [
                "condition_success",
                "placement_success"
            ]
        }
    }
}
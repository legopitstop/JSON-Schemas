{
    "id": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock/behavior/aggregate_feature.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "minecraft:aggregate_feature": {
            "description": "'minecraft:aggregate_feature' places a collection of features in an arbitary order. All features in the collection use the same input position. Features should not depend on each other, as there is no guarantee on the order the features will be placed.Succeeds if: At lease one feature is placed successfully.Fails if: All features fail to be placed.",
            "type": "object",
            "default": {},
            "properties": {
                "description": {
                    "type": "object",
                    "properties": {
                        "identifier": {
                            "$ref": "https://raw.githubusercontent.com/legopitstop/JSON-Schemas/main/schemas/bedrock/common.json#/definitions/nameSpaceID"
                        }
                    }
                },
                "features": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "string"
                    }
                },
                "early_out": {
                    "enum": [
                        "none",
                        "first_failure",
                        "first_success"
                    ]
                }
            }
        }
    }
}